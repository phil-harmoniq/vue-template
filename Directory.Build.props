<Project>

  <PropertyGroup>
    <BuildPath>.vscode/build</BuildPath>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>  
    <OutputPath>$(MSBuildThisFileDirectory)/$(BuildPath)/$(MSBuildProjectName)/bin/$(Configuration)</OutputPath>
    <BaseIntermediateOutputPath>$(MSBuildThisFileDirectory)/$(BuildPath)/$(MSBuildProjectName)/obj</BaseIntermediateOutputPath>
  </PropertyGroup>

</Project>
